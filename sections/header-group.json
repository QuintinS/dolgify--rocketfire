{
  "type": "header",
  "name": "Header group",
  "sections": {
    "announcement-bar": {
      "type": "announcement-bar",
      "blocks": {
        "message-1": {
          "type": "message",
          "settings": {
            "text": "FREE SHIPPING ON NEW ORDERS (in the Continental US)!",
            "learn_more_text": "",
            "title": "",
            "content": "",
            "button_text": "",
            "button_link": ""
          }
        }
      },
      "block_order": [
        "message-1"
      ],
      "custom_css": [
        "p {font-weight: bold; font-family: Fjalla One;}"
      ],
      "settings": {
        "announcement_bar_position": "non_sticky",
        "announcement_text_size": "normal",
        "autoplay": false,
        "cycle_speed": 5,
        "background": "#161616",
        "text_color": "#ffffff",
        "button_background": "#ffffff",
        "button_text_color": "#ffffff"
      }
    },
    "header": {
      "type": "header",
      "custom_css": [
        "a {font-family: \"Fjalla One\"; font-size: 18px; font-weight: 600;}",
        ".header__linklist-item:last-child a {background: #f25a26; padding: 0.5em 1em; border-radius: var(--button-border-radius); color: #fff;}",
        ".header__linklist {align-items: center;}"
      ],
      "settings": {
        "enable_sticky_header": true,
        "logo": "{{ shop.brand.logo }}",
        "logo_max_width": 140,
        "mobile_logo_max_width": 100,
        "navigation_menu": "new-menu",
        "sidebar_navigation_menu": "mobile-menu",
        "header_layout": "logo_left_navigation_inline",
        "show_icons": true,
        "reduce_desktop_padding": false,
        "show_locale_selector": true,
        "show_country_selector": false,
        "enable_transparent_header": true,
        "transparent_header_text_color": "#ffffff",
        "search_menu": ""
      }
    }
  },
  "order": [
    "announcement-bar",
    "header"
  ]
}